{
  "name": "kory-ui",
  "version": "0.0.1",
  "scripts": {
    "dev": "cross-env  --max_old_space_size=4096  vite --host",
    "build": "vite build",
    "report": "cross-env REPORT=true npm run build",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "npx rimraf node_modules",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "test:gzip": "http-server dist --cors --gzip -c-1",
    "test:br": "http-server dist --cors --brotli -c-1",
    "reinstall": "rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && yarn",
    "update": "yarn upgrade-interactive --latest",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "install:husky": "is-ci || husky install",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "@css-render/plugin-bem": "^0.15.5",
    "@css-render/vue3-ssr": "^0.15.5",
    "async-validator": "^3.5.2",
    "css-render": "^0.15.5",
    "date-fns": "^2.23.0",
    "highlight.js": "^11.1.0",
    "lodash-es": "^4.17.21",
    "path-to-regexp": "^6.2.0",
    "qs": "^6.10.1",
    "seemly": "^0.3.1",
    "sortablejs": "^1.14.0",
    "vfonts": "^0.1.0",
    "vooks": "^0.2.8",
    "vue": "^3.1.5",
    "vue-router": "^4.0.10",
    "vueuc": "^0.4.9"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@babel/generator": "^7.14.8",
    "@babel/parser": "^7.14.8",
    "@babel/traverse": "^7.14.8",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@iconify/json": "^1.1.380",
    "@purge-icons/generated": "^0.7.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@types/lodash-es": "^4.17.4",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "@vitejs/plugin-legacy": "^1.5.0",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vitejs/plugin-vue-jsx": "^1.1.7",
    "@vue/compiler-sfc": "^3.1.5",
    "autoprefixer": "^10.3.1",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.0.9",
    "eslint-plugin-markdown": "^2.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.14.0",
    "esno": "^0.8.0",
    "is-ci": "^3.0.0",
    "lint-staged": "^11.1.1",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.5.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "vite": "^2.4.4",
    "vue-eslint-parser": "^7.10.0",
    "vue-tsc": "^0.1.7"
  },
  "engines": {
    "node": "^12 || >=14"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.tsx": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "prettier --parser=vue --write",
      "eslint --fix"
    ],
    "*.css": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write --parser markdown --prose-wrap never",
      "eslint --fix"
    ]
  }
}
