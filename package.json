{
  "name": "kory-admin",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build && esno ./build/script/index.ts",
    "build:example": "cross-env vite build --mode example && esno ./build/script/index.ts",
    "report": "cross-env REPORT=true npm run build",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:lint-staged": "lint-staged",
    "test:gzip": "http-server dist --cors --gzip -c-1",
    "test:br": "http-server dist --cors --brotli -c-1",
    "reinstall": "rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && pnpm i",
    "update": "yarn upgrade-interactive --latest",
    "preview": "pnpm run build && vite preview",
    "preview:dist": "vite preview",
    "install:husky": "is-ci || husky install",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "@ant-design/colors": "^6.0.0",
    "@ant-design/icons-vue": "^6.1.0",
    "@iconify/iconify": "^2.2.0",
    "@vueuse/core": "^8.2.5",
    "@vueuse/shared": "^8.4.1",
    "ant-design-vue": "^3.2.3",
    "axios": "^0.27.2",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.0",
    "echarts": "^5.3.2",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.2.0",
    "pinia": "^2.0.13",
    "qrcode": "^1.5.0",
    "qs": "^6.10.3",
    "resize-observer-polyfill": "^1.5.1",
    "sortablejs": "^1.15.0",
    "tinymce": "5.10.3",
    "vditor": "^3.8.13",
    "vue": "^3.2.33",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.0.14",
    "xlsx": "^0.18.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@iconify/json": "^2.1.25",
    "@purge-icons/generated": "^0.8.1",
    "@types/crypto-js": "^4.0.1",
    "@types/fs-extra": "^9.0.11",
    "@types/lodash-es": "^4.17.5",
    "@types/mockjs": "^1.0.3",
    "@types/nprogress": "^0.2.0",
    "@types/qrcode": "^1.4.0",
    "@types/qs": "^6.9.6",
    "@types/yargs": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-legacy": "^1.4.1",
    "@vitejs/plugin-vue": "^2.3.2",
    "@vitejs/plugin-vue-jsx": "^1.1.5",
    "@vue/compiler-sfc": "^3.2.33",
    "autoprefixer": "^10.2.6",
    "commitizen": "^4.2.3",
    "consola": "^2.15.3",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "dot-prop": "^7.2.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.0.8",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "esno": "^0.14.1",
    "fs-extra": "^10.1.0",
    "http-server": "^14.1.0",
    "is-ci": "^3.0.0",
    "less": "^4.1.1",
    "lint-staged": "^12.3.7",
    "mockjs": "^1.1.0",
    "postcss": "^8.3.0",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.72.0",
    "rollup-plugin-visualizer": "^5.5.0",
    "stylelint": "^15.10.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.3.2",
    "unplugin-vue-components": "^0.19.3",
    "vite": "2.9.8",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-mkcert": "^1.6.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-purge-icons": "^0.8.1",
    "vite-plugin-pwa": "^0.12.0",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-theme": "^0.8.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vite-plugin-windicss": "^1.8.3",
    "vue-eslint-parser": "^8.3.0",
    "vue-tsc": "^0.34.11"
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china",
    "rollup": "^2.56.3",
    "gifsicle": "5.2.0"
  },
  "engines": {
    "node": "^12 || >=14"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,less,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
